meta {
  name: Random Fields
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}?elementType=entries&id=48&fields=address%2Casset&one=1
  body: none
  auth: inherit
}

params:query {
  elementType: entries
  id: 48
  fields: address%2Casset
  one: 1
}

assert {
